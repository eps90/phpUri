{
    "name": "jakub.turek/phpuri",
    "description": "URI parser and validator",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "silex/silex": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "symfony/browser-kit": "^3.2",
        "symfony/css-selector": "^3.2"
    },
    "authors": [
        {
            "name": "Jakub Turek",
            "email": "ja@kubaturek.pl"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "EPS\\PhpUri\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EPS\\PhpUri\\Tests\\": "tests/"
        }
    }
}
